def f():
    if get_surface_color() == BLACK:
        color(0, 0, 0)
    if get_surface_color() == RED:
        color(127, 0, 0)
    if get_surface_color() == GREEN:
        color(0, 127, 0)
    if get_surface_color() == BLUE:
        color(0, 0, 127)
    if get_surface_color() == WHITE:
        color(127, 127, 127)
    if get_surface_color() == YELLOW:
        color(127, 127, 0)
    if get_surface_color() == CYAN:
        color(0, 127, 127)
    if get_surface_color() == MAGENTA:
        color(127, 0, 127)

while True:
    f()
